partial modifier_keys xkb_symbols "empty" {
};

partial modifier_keys xkb_symbols "caps" {
    key <CAPS> {	[	Caps_Lock		]	};
};

partial modifier_keys xkb_symbols "lshift" {
    key <LFSH> {	[ 	 Shift_L		]	};
};

partial modifier_keys xkb_symbols "rshift" {
    key <RTSH> {	[ 	Shift_R		]	};
};

partial modifier_keys xkb_symbols "lctrl" {
    key <LCTL> {	[ 	 Control_L		]	};
};

partial modifier_keys xkb_symbols "rctrl" {
    key <RCTL> {	[ 	Control_R	]	};
};

partial modifier_keys xkb_symbols "lalt" {
    key <LALT> {	[ 	Alt_L	 	]	};
};

partial modifier_keys xkb_symbols "ralt" {
    key <RALT> {	[	Alt_R		]	};
};

partial modifier_keys xkb_symbols "lwin" {
    key <LWIN> {	[	Super_L			]	};
};

partial modifier_keys xkb_symbols "rwin" {
    key <RWIN> {	[	Super_R			]	};
};

partial modifier_keys xkb_symbols "menu" {
    key <MENU> {	[	Menu			]	};
};

partial modifier_keys xkb_symbols "sclk" {
    key <SCLK> {  [  Scroll_Lock	]	};
};
